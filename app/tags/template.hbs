<h1>Tags</h1>

{{#paper-content class="md-whiteframe-z1 list-demo"}}
  {{#paper-list}}
    {{#each model as |tag|}}
      {{#paper-item class="md-3-line"}}
          <div class="md-list-item-text">
              <h3>ID: {{tag.id}}</h3>
              <h3>Name: {{tag.name}}</h3>
              <p>Posts:
                {{#each tag.posts as |post|}}
                  {{post.title}}
                {{/each}}
              </p>
          </div>
      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
{{/paper-content}}

{{outlet}}